<!DOCTYPE html>
<html>
  <head>
    <title>Dc</title>
    <%= csrf_meta_tags %>
    <%= action_cable_meta_tag %>
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous">
    <link href="https://fonts.googleapis.com/css?family=Lato:100,100i,300,300i,400,400i,700,700i,900,900i" rel="stylesheet">    
    <%= stylesheet_link_tag    'client', media: 'all' %>
  </head>

  <body>

    <div id="wrapper">
      <%= render partial: "partials/navigation/sidebar_navigation_client" %>
        <div id="page-wrapper" class="gray-bg">
          <div class="row border-bottom">
          <%= render partial: "partials/navigation/top_navigation_client"%>


        <div class="row">
            <div class="col-lg-12">
                <div class="wrapper wrapper-content">
                <%= yield %>
                </div>
            </div>
        </div>
        
          <%= render partial: "partials/footer/footer_client" %>

          </div>
        </div>
    </div>

      
      
      <%= javascript_include_tag 'client' %>
  </body>

</html>
